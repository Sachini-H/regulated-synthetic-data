        "# Function to determine the date based on a time threshold\n",
        "def get_factory_date(dt):\n",
        "    dt = dt + timedelta(hours=hour_offset)\n",
        "    if dt.time() < pd.to_datetime('05:00:00').time():\n",
        "        return (dt - timedelta(days=1)).date()\n",
        "    else:\n",
        "        return dt.date()"
      ],
